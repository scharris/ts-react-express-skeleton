{
   "name": "server",
   "version": "1.0.0",
   "description": "Server side of skeleton project.",
   "scripts": {
      "gen-dbmd": "dbmd-gen-pg --conn-env envs/pg-local.env query-gen/dbmd.json",
      "gen-queries": "npm run compile-qgen && npm run qgen",
      "compile-qgen": "cd query-gen && npm install && npx tsc",
      "qgen": "node query-gen/run.js --dbmd query-gen/dbmd.json --types-header query-gen/types-header --ts-output-dir src/generated/ts --sql-output-dir src/generated/sql",
      "build-client-prod": "cd ../client && npm run build",
      "build-server-prod": "webpack --config webpack/prod.config.js",
      "prebuild": "npm run gen-queries",
      "build": "npm run build-client-prod && npm run build-server-prod",
      "start": "node dist/server dist envs/pg-local-drugs.env",
      "build-start": "npm run build && npm run start",
      "prebuild-server-dev": "npm run gen-queries",
      "build-server-dev": "webpack --config webpack/dev.nowatch.config.js",
      "build-start-server-dev": "npm run build-server-dev && npm run start",
      "start-client-dev": "cd ../client && npm run start",
      "dev": "npm run build-server-dev && concurrently \"npm:start-client-dev\" \"npm:start\""
   },
   "keywords": [],
   "author": "",
   "license": "MIT",
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.1.1",
      "pg": "^8.4.1",
      "pg-pool": "^3.2.1"
   },
   "devDependencies": {
      "@types/cors": "^2.8.7",
      "@types/express": "^4.17.8",
      "@types/helmet": "0.0.48",
      "@types/node": "^14.11.5",
      "@types/pg": "^7.14.5",
      "@types/pg-pool": "^2.0.2",
      "clean-webpack-plugin": "^3.0.0",
      "concurrently": "^5.3.0",
      "copy-webpack-plugin": "^6.2.0",
      "npm-run-all": "^4.1.5",
      "ts-loader": "^8.0.4",
      "typescript": "^4.0.3",
      "webpack": "^4.44.2",
      "webpack-cli": "^3.3.12",
      "webpack-merge": "^5.2.0",
      "webpack-node-externals": "^2.5.2"
   }
}
