{
   "name": "server",
   "version": "1.0.0",
   "description": "",
   "main": "server.js",
   "scripts": {
      "build-client-prod": "cd ../client && npm run build",
      "build-server-prod": "webpack --config webpack/prod.config.js",
      "prebuild": "powershell dagen/generate-sources -DagenVer 1.5 -GeneratedSourceRoot src/generated",
      "build": "npm run build-client-prod && npm run build-server-prod",
      "start": "node dist/server dist envs/pg-local-drugs.env",
      "build-start": "npm run build && npm run start",
      "build-server-dev": "webpack --config webpack/dev.nowatch.config.js",
      "start-client-dev": "cd ../client && npm run start",
      "dev": "npm run build-server-dev && concurrently \"npm:start-client-dev\" \"npm:start\"",
      "fetch-dbmd": "powershell dagen/dbmd/fetch -DagenVer 1.5"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.1.1",
      "pg": "^8.4.1",
      "pg-pool": "^3.2.1"
   },
   "devDependencies": {
      "@types/cors": "^2.8.7",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.8",
      "@types/helmet": "0.0.48",
      "@types/node": "^14.11.5",
      "@types/pg": "^7.14.5",
      "@types/pg-pool": "^2.0.2",
      "clean-webpack-plugin": "^3.0.0",
      "concurrently": "^5.3.0",
      "copy-webpack-plugin": "^6.2.0",
      "npm-run-all": "^4.1.5",
      "ts-loader": "^8.0.4",
      "typescript": "^4.0.3",
      "webpack": "^4.44.2",
      "webpack-cli": "^3.3.12",
      "webpack-merge": "^5.2.0",
      "webpack-node-externals": "^2.5.2"
   }
}
